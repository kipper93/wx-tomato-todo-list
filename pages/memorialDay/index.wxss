.content{
    width: 100%;
    height: 100vh;
    background-size: cover;
    background-repeat: no-repeat;
    background:repeating-linear-gradient(to right,rgb(229, 230, 218),rgb(228, 219, 219));
}

.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.page-section-spacing{
    margin-top: 60rpx;
  }
  .scroll-view_H{
    white-space: nowrap;
  }
  .scroll-view-item{
    height: 100rpx;
  }
  .scroll-view-item_H{
      margin-left: 20rpx;
    display: inline-block;
    width: 50%;
    height: 100rpx;
    background-color: red;
    text-align: center;
  }

  .birthday_list{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 14rpx;
    font-size: 24rpx;
    color:orangered;
  }
  .birthday_name{
    display: flex;
    align-items: center;
    color:orangered;
    width:30%;
  }
  .birthday_date{
    width:30%;
  }
  .birthday_day{
    width:24%;
  }
  .birthday_name image{
    width: 48rpx;
    height: 48rpx;
  }
  swiper{
    height: 600rpx;
  }
.swiper-item {
  height: 100%;
  width:100%;
  object-fit: contain;
}
.text_btn{
    color: red;
    font-size: 20rpx;
    padding: 16rpx;
    
}
.time{
    display: flex;
    justify-content: space-between;

}
